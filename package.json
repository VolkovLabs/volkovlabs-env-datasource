{
  "author": "Volkov Labs",
  "dependencies": {
    "@emotion/css": "^11.11.2",
    "@grafana/data": "^11.1.0",
    "@grafana/runtime": "^11.3.0",
    "@grafana/schema": "^11.1.0",
    "@grafana/ui": "^11.1.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "tslib": "^2.8.1"
  },
  "description": "Data Source to return environment variables for Grafana",
  "devDependencies": {
    "@babel/core": "^7.26.0",
    "@grafana/eslint-config": "^8.0.0",
    "@grafana/plugin-e2e": "^1.11.2",
    "@grafana/tsconfig": "^2.0.0",
    "@playwright/test": "^1.48.2",
    "@swc/core": "^1.7.42",
    "@swc/helpers": "^0.5.13",
    "@swc/jest": "^0.2.37",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.0.1",
    "@types/jest": "^29.5.14",
    "@types/lodash": "^4.17.13",
    "@types/node": "^22.8.7",
    "@typescript-eslint/eslint-plugin": "^8.12.2",
    "copy-webpack-plugin": "^12.0.2",
    "css-loader": "^7.1.2",
    "eslint-plugin-deprecation": "^3.0.0",
    "eslint-plugin-react": "^7.37.2",
    "eslint-webpack-plugin": "^4.2.0",
    "fork-ts-checker-webpack-plugin": "^9.0.2",
    "glob": "^11.0.0",
    "identity-obj-proxy": "3.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "prettier": "^3.3.3",
    "replace-in-file-webpack-plugin": "^1.0.6",
    "sass": "^1.80.6",
    "sass-loader": "^16.0.3",
    "style-loader": "^4.0.0",
    "swc-loader": "^0.2.6",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "5.6.3",
    "webpack": "^5.96.1",
    "webpack-cli": "^5.1.4",
    "webpack-livereload-plugin": "^3.0.2"
  },
  "engines": {
    "node": ">=20"
  },
  "license": "Apache-2.0",
  "name": "volkovlabs-env-datasource",
  "scripts": {
    "build": "webpack -c ./.config/webpack/webpack.config.ts --env production",
    "build:backend": "mage -v lint && mage -v coverage && mage -v",
    "dev": "webpack -w -c ./.config/webpack/webpack.config.ts --env development",
    "levitate": "npx @grafana/levitate@latest is-compatible --path src/module.ts --target @grafana/data,@grafana/runtime,@grafana/ui",
    "lint": "eslint --ignore-path ./.eslintignore --ext .js,.jsx,.ts,.tsx .",
    "lint:fix": "eslint --fix --ignore-path ./.eslintignore --ext .js,.jsx,.ts,.tsx .",
    "restart:docker:plugin": "docker exec -it grafana pkill -f volkovlabs-env-datasource",
    "sign": "npx --yes @grafana/sign-plugin@latest --rootUrls http://localhost:3000/",
    "start": "docker compose pull grafana && docker compose --profile dev up",
    "start:main": "docker compose pull grafana-main && docker compose --profile main up",
    "stop": "docker compose down",
    "test": "jest --watch --onlyChanged",
    "test:e2e": "npx playwright test",
    "test:e2e:docker": "docker compose --profile e2e up --exit-code-from test",
    "test:ci": "jest --maxWorkers 4 --coverage",
    "upgrade": "npm upgrade --save"
  },
  "version": "4.1.0"
}
